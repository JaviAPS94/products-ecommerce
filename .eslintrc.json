{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreComments": true
      }
    ],
    "newline-before-return": "error",
    "implicit-arrow-linebreak": "off"
  }
}